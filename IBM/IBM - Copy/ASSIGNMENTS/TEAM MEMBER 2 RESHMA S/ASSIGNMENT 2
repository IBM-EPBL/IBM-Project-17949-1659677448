import random
import time
while(10):
    temp = random.randint(0,200)
    h = random.randint(0,100) 
    d = (5*(temp-32)/9 )
    print("temp in celsius :",d)
    time.sleep(2)
    print("relative humidity value  :",h,"%")
    time.sleep(2)
    if(d>100):
        print("Not allowed: WARNING!!!")
    else:
        print("u are allowed")
