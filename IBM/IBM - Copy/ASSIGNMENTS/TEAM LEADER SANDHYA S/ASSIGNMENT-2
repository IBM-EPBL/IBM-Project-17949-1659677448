import random
import time
while(1000>100):
    temp = random.randint(0,100)
    humidity = random.randint(0,100) 
     
    print("temp in celsius :",temp,"Â°c")
    print("relative humidity value  :",humidity,"%")

    if(temp>24 & humidity<50):
     
        print("alarm detected")
    time.sleep(2)
