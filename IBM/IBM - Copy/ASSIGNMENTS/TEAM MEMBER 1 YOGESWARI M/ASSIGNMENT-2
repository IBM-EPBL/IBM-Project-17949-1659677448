import random
import time
while(10):
    time.sleep(1)
    temp = random.randint(0,300)
    rh = random.randint(0,100) 
    c = (5*(temp-32)/9 )
    print("temp in celsius :",c)
    time.sleep(2)
    print("relative humidity value  :",rh,"%")
    time.sleep(2)
    if(c>100):
        print("HIGH TEMPERATURE WARNING!!!")
    else:
        print("GOOD CONDITION")
